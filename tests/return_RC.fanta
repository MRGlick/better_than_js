
struct Vec2 {
    float x;
    float y;
}

void print_vec(Vec2 a) 
    print "(", a.x, ", ", a.y, ")";

Vec2 add(Vec2 a, Vec2 b) {
    Vec2 c = new Vec2(
        x = a.x + b.x,
        y = a.y + b.y
    );

    return c;
}
Vec2 a = new Vec2(x = 13, y = 14);

print_vec(add(a, new Vec2(x = 5, y = 6)));