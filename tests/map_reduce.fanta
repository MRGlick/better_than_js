


int map_reduce(int[] arr, int(int) mapper, int(int, int) reducer) {
    
    if (arr.length == 0) {
        print "Cannot map reduce an empty array!";
        return -1;
    }
    
    int[] temp = [arr.length; ];

    for (int i = 0; i < temp.length; i += 1; ) {
        temp[i] = mapper(arr[i]);
    }

    int res = temp[0];

    for (int i = 1; i < arr.length; i += 1; ) {
        res = reducer(res, temp[i]);
    }

    return res;
}

int double(int a) return a * 2;

int add(int a, int b) return a + b;

print map_reduce([1, 2, 3, 4, 5], double, add);