struct Node {
    Node next;
    int val;
}

void print_node(Node n) print "Node[val = ", n.val, "]";

void print_nodes(Node n) {
    Node curr = n;
    while (curr != null) {
        print_node(curr);
        curr = curr.next;
    }
}

Node lst = new Node(
    val = 5, 
    next = new Node(
        val = 7, 
        next = new Node(
            val = 9, 
            next = new Node(
                val = 10,
                next = new Node(
                    val = 11
                )
            )
        )
    )
);

print_nodes(lst);

