struct Vec2 {
    float x;
    float y;
}

Vec2 add(Vec2 a, Vec2 b) {
    return new Vec2(x = a.x + b.x, y = a.y + b.y);
}

void print_vec(Vec2 a) {
    print "(", a.x, ", ", a.y, ")";
}

Vec2 a = new Vec2(
    x = 5,
    y = 6
);

Vec2 b = new Vec2(
    x = 67,
    y = 1
);

Vec2 c = add(a, b);

print_vec(c);