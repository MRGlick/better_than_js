int num;

print "Enter number: ";
input num;

int i = 1;
bool prime = true;

if (num == 1) prime = false;

while (i < num / 2 && prime) {
    i = i + 1;
    print num, " % ", i, " = ", num % i;
    if (num % i == 0) prime = false;
}

if (prime)
    print num, " is a prime number!";
 else 
    print num, num, num, num, num;
